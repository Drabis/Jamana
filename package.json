    "connect-mongodb-session": "^2.4.1",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "mongoose": "^5.12.14",
    "nodemon": "^2.0.7",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "concurrently": "^6.2.0",
    "if-env": "^1.0.4"
  }
}
